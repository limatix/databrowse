<xsl:template xmlns="http://www.w3.org/1999/xhtml" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:dc="http://thermal.cnde.iastate.edu/datacollect" xmlns:dcv="http://thermal.cnde.iastate.edu/dcvalue" xmlns:chx="http://thermal.cnde.iastate.edu/checklist" xmlns:xlink="http://www.w3.org/1999/xlink" match="dc:measurement" mode="full">
    <h3>Measurement Summary</h3>
    <table class="cream full-width">
            <thead>
                <tr><th style="width:166px">Item</th><th>Value</th></tr>
            </thead>
            <tbody>
                <tr><th><abbr title="dc:measnum">Measurement #</abbr></th><td><xsl:value-of select="dc:measnum"/></td></tr>
                <xsl:if test="dc:measphotos and count(dc:measphotos/*) > 0">
                    <tr><th><abbr title="dc:measphotos">Measurement Photos</abbr></th><td>
                        <ul class="inlinelist">
                            <xsl:for-each select="dc:measphotos/dcv:photo">
                                <li>
                                    <a>
                                    <xsl:choose>
                                        <xsl:when test="@xlink:href and not(@xlink:href='')">
                                            <xsl:attribute name="href"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="@xlink:href"/></xsl:attribute>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <xsl:attribute name="href"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="."/></xsl:attribute>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <img>
                                        <xsl:attribute name="src"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:choose><xsl:when test="@xlink:href and not(@xlink:href='')"><xsl:value-of select="@xlink:href"/></xsl:when><xsl:otherwise><xsl:value-of select="."/></xsl:otherwise></xsl:choose>?content_mode=raw&amp;thumbnail=small</xsl:attribute>
                                    </img>
                                </a>
                                </li>
                            </xsl:for-each>
                        </ul>
                    </td></tr>
                </xsl:if>
                <xsl:choose>
                    <xsl:when test="dc:specimen/@url">
                        <tr><th><abbr title="dc:specimen">Specimen</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="dc:specimen/@url"/></xsl:attribute><xsl:value-of select="dc:specimen"/></a></td></tr>
                    </xsl:when>
                    <xsl:otherwise>
                        <tr><th><abbr title="dc:specimen">Specimen</abbr></th><td><xsl:value-of select="dc:specimen"/></td></tr>
                    </xsl:otherwise>
                </xsl:choose>
                <tr><th><abbr title="dc:date">Date</abbr></th><td><xsl:value-of select="dc:date"/></td></tr>
                <tr><th><abbr title="dc:perfby">Performed By</abbr></th><td><xsl:value-of select="dc:perfby"/></td></tr>
                <tr><th><abbr title="dc:clinfo">Checklist Name</abbr></th><td><xsl:value-of select="chx:clinfo"/></td></tr>
                <tr><th><abbr title="dc:cltitle">Checklist Title</abbr></th><td><xsl:value-of select="chx:cltitle"/></td></tr>
                <xsl:if test="dc:measchecklist and not(dc:measchecklist='')"><tr><th><abbr title="dc:measchecklist">Checklist Link</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="dc:measchecklist"/></xsl:attribute><xsl:value-of select="dc:measchecklist"/></a></td></tr></xsl:if>
                <xsl:if test="dc:measchecklist/@xlink:href and not(dc:measchecklist/@xlink:href='')"><tr><th><abbr title="dc:measchecklist">Checklist Link</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="dc:measchecklist/@xlink:href"/></xsl:attribute><xsl:value-of select="dc:measchecklist/@xlink:href"/></a></td></tr></xsl:if>
                <xsl:if test="dc:dgsfile and not(dc:dgsfile='')"><tr><th><abbr title="dc:dgsfile">DGS File</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="dc:dgsfile"/></xsl:attribute><xsl:value-of select="dc:dgsfile"/></a><p style="text-align:center"><img width="75%"><xsl:attribute name="src"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="dc:dgsfile"/>?content_mode=raw&amp;image=image&amp;snapshot=1&amp;waveform=IRimg</xsl:attribute></img></p></td></tr></xsl:if>
                <xsl:if test="dc:dgsfile/@xlink:href and not(dc:dgsfile/@xlink:href='')"><tr><th><abbr title="dc:dgsfile">DGS File</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="dc:dgsfile/@xlink:href"/></xsl:attribute><xsl:value-of select="substring-after(dc:dgsfile/@xlink:href,'/')"/></a><p style="text-align:center"><img width="75%"><xsl:attribute name="src"><xsl:value-of select="dc:dgsfile/@xlink:href"/>?content_mode=raw&amp;image=image&amp;snapshot=1&amp;waveform=IRimg</xsl:attribute></img></p></td></tr></xsl:if>
                <xsl:if test="dc:scanxy_outdgd and not(dc:scanxy_outdgd='')"><tr><th><abbr title="dc:scanxy_outdgd">ScanXY DGD File</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="dc:scanxy_outdgd"/></xsl:attribute><xsl:value-of select="dc:scanxy_outdgd"/></a></td></tr></xsl:if>      
                <xsl:if test="dc:scan_outdgd and not(dc:scan_outdgd='')"><tr><th><abbr title="dc:scan_outdgd">Scan DGD File</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="dc:scan_outdgd"/></xsl:attribute><xsl:value-of select="dc:scan_outdgd"/></a></td></tr></xsl:if>  

                <xsl:if test="dc:stitchedimagesfolder and not(dc:stitchedimagesfolder='')"><tr><th><abbr title="dc:stitchedimagesfolder">Stitched Images Folder</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="dc:stitchedimagesfolder"/></xsl:attribute><xsl:value-of select="dc:stitchedimagesfolder"/></a></td></tr></xsl:if> 

                <xsl:if test="dc:rawimagesfolder and not(dc:rawimagesfolder='')"><tr><th><abbr title="dc:rawimagesfolder">Raw Images Folder</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="dc:rawimagesfolder"/></xsl:attribute><xsl:value-of select="dc:rawimagesfolder"/></a></td></tr></xsl:if> 

                <xsl:if test="dc:stitcheddgsfile and not(dc:stitcheddgsfile='')"><tr><th><abbr title="dc:stitcheddgsfile">Stitched DGS File</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="dc:stitcheddgsfile"/></xsl:attribute><xsl:value-of select="dc:stitcheddgsfile"/></a><p style="text-align:center"><img width="75%"><xsl:attribute name="src"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="dc:stitcheddgsfile"/>?content_mode=raw&amp;image=image&amp;snapshot=1&amp;waveform=GEVProc&amp;frame=0</xsl:attribute></img></p></td></tr></xsl:if>

                <xsl:if test="dc:setfile and not(dc:setfile='')"><tr><th><abbr title="dc:setfile">Set File</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="dc:setfile"/></xsl:attribute><xsl:value-of select="dc:setfile"/></a></td></tr></xsl:if>
                <xsl:if test="dc:setfile/@xlink:href and not(dc:setfile/@xlink:href='')"><tr><th><abbr title="dc:setfile">Set File</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="dc:setfile/@xlink:href"/></xsl:attribute><xsl:value-of select="substring-after(dc:setfile/@xlink:href,'/')"/></a></td></tr></xsl:if>
                <tr><th><abbr title="dc:notes">Notes</abbr></th><td><xsl:call-template name="dc2-lf2p">
                        <xsl:with-param name="instr">
                            <xsl:value-of select="dc:notes"/>
                        </xsl:with-param>
                    </xsl:call-template></td></tr>
                <xsl:for-each select="*[not(self::dc:measnum or self::chx:clinfo or self::chx:cltitle or self::dc:measphotos or self::dc:specimen or self::dc:date or self::dc:perfby or self::dc:measchecklist or self::dc:notes or self::dc:setfile or self::dc:dgsfile or self::dc:scanxy_outdgd or self::dc:scan_outdgd or self::dc:stitchedimagesfolder or self::dc:rawimagesfolder or self::dc:stitcheddgsfile)]">
                    <xsl:sort data-type="text" select="local-name()"/>
                    <xsl:variable name="localname"><xsl:value-of select="local-name()"/></xsl:variable>
                    <xsl:choose>
                        <xsl:when test="$localname = 'xducer' and @url">
                            <tr><th><abbr title="dc:xducer">Transducer</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="@url"/></xsl:attribute><xsl:value-of select="."/></a></td></tr>
                        </xsl:when>
                        <xsl:when test="$localname = 'undistorted_png'">
                            <tr><th><abbr title="dc:undistorted_png">Undistorted PNG</abbr></th><td><div style="word-wrap:break-word;"><a><xsl:attribute name="href"><xsl:value-of select="@xlink:href"/></xsl:attribute><xsl:value-of select="@xlink:href"/></a></div><br/><div style="text-align:center"><img><xsl:attribute name="src"><xsl:value-of select="@xlink:href"/>?content_mode=raw&amp;thumbnail=large</xsl:attribute></img></div><br/><table class="cream"><thead><tr><th colspan="2">Image Information</th></tr></thead><tbody><tr><th>Units/Intensity</th><td><xsl:value-of select="@unitsperintensity"/></td></tr><tr><th>Frame Time</th><td><xsl:value-of select="@frametime"/></td></tr><tr><th>Channel</th><td><xsl:value-of select="@channel"/></td></tr><tr><th>Offset</th><td><xsl:value-of select="@offset"/></td></tr></tbody></table></td></tr>
                        </xsl:when>
                        <xsl:when test="$localname = 'vibropowerplot'">
                            <tr><th><abbr title="dc:vibropowerplot">Thermal Power Plot</abbr></th><td><a><xsl:attribute name="href"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="."/></xsl:attribute><xsl:value-of select="."/></a><br/><div style="text-align:center"><img><xsl:attribute name="src"><xsl:value-of select="//dc:experiment/@reldesturl"/>/<xsl:value-of select="."/>?content_mode=raw&amp;thumbnail=large</xsl:attribute></img></div></td></tr>
                        </xsl:when>
                        <xsl:when test="$localname = 'simulationoutput'">
                            <tr><th><abbr title="dc:simulationoutput">Simulation Output</abbr></th><td>
                                <xsl:if test="not(. = '')"><pre class="prettyprint dgcode">
                                    <xsl:value-of select="."/>
                                </pre></xsl:if>
                            </td></tr>
                        </xsl:when>
                        <xsl:when test="contains($localname, '_out')">
                            <tr>
                                <th><abbr><xsl:attribute name="title"><xsl:value-of select="name()"/></xsl:attribute>
                                    <xsl:choose>
                                        <xsl:when test="document('')/*/db:fieldmap/db:entry[@key=$localname]">
                                            <xsl:value-of select="document('')/*/db:fieldmap/db:entry[@key=$localname]"/>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <xsl:value-of select="$localname"/>                                    
                                        </xsl:otherwise>
                                    </xsl:choose></abbr>
                                </th>
                                <td><xsl:if test="not(. = '')"><pre class="prettyprint dgcode">
                                    <xsl:value-of select="."/>
                                </pre></xsl:if></td>
                            </tr>
                        </xsl:when>
                        <xsl:when test="$localname = 'opticalregistration'">
                            <tr><th><abbr title="dc:opticalregistration">Optical Registration</abbr></th><td>
                                <table class="cream full-width">
                                    <tbody>
                                        <tr><th>Reference Frame</th><td colspan="2"><xsl:value-of select="@dc:refframe"/></td></tr>
                                        <tr><th>Crack Position</th><th>X Position</th><th>Y Position</th></tr>
                                        <tr><th>Start</th><td><xsl:value-of select="dc:crackposition/dc:startx"/>&#160;<xsl:value-of select="dc:crackposition/dc:startx/@dcv:units" /></td><td><xsl:value-of select="dc:crackposition/dc:starty"/>&#160;<xsl:value-of select="dc:crackposition/dc:starty/@dcv:units" /></td></tr>
                                        <tr><th>End</th><td><xsl:value-of select="dc:crackposition/dc:endx"/>&#160;<xsl:value-of select="dc:crackposition/dc:endx/@dcv:units" /></td><td><xsl:value-of select="dc:crackposition/dc:endy"/>&#160;<xsl:value-of select="dc:crackposition/dc:endy/@dcv:units" /></td></tr>
                                    </tbody>
                                </table>
                                <table class="cream opticalregistration">
                                <xsl:attribute name="id">datatable_opticalregistration_<xsl:number/></xsl:attribute>
                                    <thead>
                                        <tr>
                                            <th>Frame #</th><th>Shift X</th><th>Shift Y</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <xsl:for-each select="dc:registrations/dc:registration">
                                            <tr>
                                                <td><xsl:value-of select="@dc:frameno"/></td><td><xsl:value-of select="dc:shiftx"/>&#160;<xsl:value-of select="dc:shiftx/@dcv:units"/></td><td><xsl:value-of select="dc:shifty"/>&#160;<xsl:value-of select="dc:shifty/@dcv:units"/></td>
                                            </tr>
                                        </xsl:for-each>
                                    </tbody>
                                </table>-->
                            </td></tr>
                        </xsl:when>
                        <xsl:when test="$localname = 'opticaltrace'">
                            <tr><th><abbr title="dc:opticaltrace">Optical Trace</abbr></th><td>
                                <table class="cream full-width">
                                    <tbody>
                                        <tr><th>Reference Frame</th><td colspan="2"><xsl:value-of select="@dc:refframe"/></td></tr>
                                    </tbody>
                                </table>
                                <table class="cream opticaltrace">
                                <xsl:attribute name="id">datatable_opticaltrace_<xsl:number/></xsl:attribute>
                                    <thead>
                                        <tr>
                                            <th>X Position</th><th>Y Position</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <xsl:for-each select="dc:cracktrace/dc:keypoint">
                                            <tr>
                                                <td><xsl:value-of select="dc:xpos"/>&#160;<xsl:value-of select="dc:xpos/@dcv:units"/></td><td><xsl:value-of select="dc:ypos"/>&#160;<xsl:value-of select="dc:ypos/@dcv:units"/></td>
                                            </tr>
                                        </xsl:for-each>
                                    </tbody>
                                </table>-->
                            </td></tr>
                        </xsl:when>
                        <xsl:when test="$localname = 'excitation'">
                            <tr><th><abbr title="dc:excitation">Excitation</abbr></th><td>
                                <xsl:choose>     
                                    <xsl:when test="@dcv:exctype='sweep'">
                                        SWEEP&#160;<xsl:value-of select="dcv:f0/." />&#160;<xsl:value-of select="dcv:f0/@dcv:units" />&#160;<xsl:value-of select="dcv:f1/." />&#160;<xsl:value-of select="dcv:f1/@dcv:units" />&#160;<xsl:value-of select="dcv:t0/." />&#160;<xsl:value-of select="dcv:t0/@dcv:units" />&#160;<xsl:value-of select="dcv:t1/." />&#160;<xsl:value-of select="dcv:t1/@dcv:units" />&#160;<xsl:value-of select="dcv:t2/." />&#160;<xsl:value-of select="dcv:t2/@dcv:units" />&#160;<xsl:value-of select="dcv:t3/." />&#160;<xsl:value-of select="dcv:t3/@dcv:units" />
                                    </xsl:when>
                                    <xsl:when test="@dcv:exctype='burst'">
                                        BURST&#160;<xsl:value-of select="dcv:f0/." />&#160;<xsl:value-of select="dcv:f0/@dcv:units" />&#160;<xsl:value-of select="dcv:f1/@dcv:units" />&#160;<xsl:value-of select="dcv:t0/." />&#160;<xsl:value-of select="dcv:t0/@dcv:units" />&#160;<xsl:value-of select="dcv:t1/." />&#160;<xsl:value-of select="dcv:t1/@dcv:units" />&#160;<xsl:value-of select="dcv:t2/." />&#160;<xsl:value-of select="dcv:t2/@dcv:units" />&#160;<xsl:value-of select="dcv:t3/." />&#160;<xsl:value-of select="dcv:t3/@dcv:units" />
                                    </xsl:when>
                                    <xsl:otherwise>
                                        <xsl:value-of select="."/>
                                    </xsl:otherwise>
                                </xsl:choose>
                            </td></tr>
                        </xsl:when>
			            <xsl:when test="$localname = 'autoexp'">
                            <tr><th><abbr title="dc:autoexp">Auto Table</abbr></th><td style="width:534px">
				                <table class="cream autoexp">
                                <xsl:attribute name="id">datatable_autoexp_<xsl:number/></xsl:attribute>
				                    <thead><tr>
                                        <xsl:for-each select="dc:automeas[1]/*">
                                            <xsl:variable name="autoexpname"><xsl:value-of select="local-name()"/></xsl:variable>
                                            <th>
                                                <abbr>
                                                    <xsl:attribute name="title"><xsl:value-of select="name()"/></xsl:attribute>
                                                    <xsl:choose>
                                                        <xsl:when test="document('')/*/db:fieldmap/db:entry[@key=$autoexpname]">
                                                            <xsl:value-of select="document('')/*/db:fieldmap/db:entry[@key=$autoexpname]"/>
                                                        </xsl:when>
                                                        <xsl:otherwise>
                                                            <xsl:value-of select="$autoexpname"/>                                    
                                                        </xsl:otherwise>
                                                    </xsl:choose>
                                                </abbr>
                                                <xsl:if test="normalize-space(@dcv:units)!=''">
                                                    &#160;(<xsl:value-of select="@dcv:units"/>)
                                                </xsl:if>
                                            </th>
                                        </xsl:for-each>
				                    </tr></thead>
                                    <tbody>
					                    <xsl:for-each select="dc:automeas">
					                        <tr>
                                                <xsl:for-each select="*">
                                                    <td>
                                                        <xsl:value-of select="." />
                                                    </td>                                
                                                </xsl:for-each>
                                            </tr>
                        				</xsl:for-each>
				                    </tbody>
                				</table>
                            </td></tr>
                        </xsl:when>
                        <xsl:otherwise>                            
                            <tr>
                                <th><abbr><xsl:attribute name="title"><xsl:value-of select="name()"/></xsl:attribute>
                                    <xsl:choose>
                                        <xsl:when test="document('')/*/db:fieldmap/db:entry[@key=$localname]">
                                            <xsl:value-of select="document('')/*/db:fieldmap/db:entry[@key=$localname]"/>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <xsl:value-of select="$localname"/>                                    
                                        </xsl:otherwise>
                                    </xsl:choose></abbr>
                                </th>
                                <td><xsl:value-of select="."/><xsl:if test="@dcv:units">&#160;<xsl:value-of select="@dcv:units"/></xsl:if></td>
                            </tr>
                        </xsl:otherwise>
                    </xsl:choose>
                </xsl:for-each>
            </tbody>
        </table>
</xsl:template>
